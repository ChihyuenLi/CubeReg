/* navbar.wxss */
.navbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10px;
  background-color: #fff;
  box-shadow: 0 1px 4px rgba(0, 0, 0, 0.1);
}

.logo {
  width: 40px;
  height: 40px;
}

.nav-items {
  display:flex;
  align-items: center;
  justify-content: center;
  flex-grow: 1;
}

.nav-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 20px;
  font-size: 14px;
  color: #666;
}

.icon {
  width: 24px;
  height: 24px;
  margin-bottom: 5px;
}
